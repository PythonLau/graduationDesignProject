        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.petsHospital.mapper.CardMapper">
    <select id="selectCardById" parameterType="int" resultType="com.petsHospital.domain.Card">
        select * from tb_card where id = #{id}
