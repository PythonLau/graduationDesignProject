<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.UserMapper">
    <insert id="save" parameterType="com.petsHospital.domain.User" useGeneratedKeys="true">
        insert into tb_user values(id_sequence.nextval,#{name},#{sex},#{age})
    </insert>
</mapper>